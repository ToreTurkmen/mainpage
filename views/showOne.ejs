<%- include('./components/header') %>

<% if (result) { %>
  <div class="card">
    <div class="card-header">
      <%= result._id %>
    </div>
    <div class="card-body">
      <h5 class="card-title"> <%= result.title %> </h5>
      <p class="card-text"> <%= result.article %> </p>
      <a href=/edit-article/<%= result._id %> class="btn btn-primary">Update</a>
      <a href=/delete-article/<%= result._id %> class="btn btn-secondary">Delete</a>
    </div>
  </div>
  
   


<% } else { %>
  there is no record

<% } %>



<%- include('./components/footer') %>